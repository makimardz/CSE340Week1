<% if (title) { %>
    <h1>
        <%= title %>
    </h1>
    <% } else { res.redirect('/') } %>
    
    <%- messages() %>
    
    <% if (errors) { %>
    <ul class="notice">
        <% errors.array().forEach(error=> { %>
        <li>
            <%= error.msg %>
        </li>
        <% }) %>
    </ul>
    <% } %>
    
    <div class="accountManagementContent">
        <h2>Welcome <%= firstName %></h2>
        <p>You're logged in.</p>
    
        <p><a href="/account/update/<%- accountData.account_id %>">Update Account information</a></p>
    
        <%- invManageLink %>
    </div>